{
  "name": "fond-of-spryker/checkout-page",
  "description": "Customise checkout process",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "scripts": {
    "test": "make codeception",
    "phpcs": "make phpcs",
    "phpstan": "make phpstan"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "spryker/glossary": "^3.8.0",
    "spryker/customer": "^7.26.5",
    "spryker-shop/checkout-page": "<3.19.0",
    "fond-of-spryker/country": "dev-feature/paypal-refund || dev-spryker_upgrade || ^1.0.0",
    "spryker-shop/customer-page": "^2.7.1",
    "spryker-shop/home-page": "^1.0.0",
    "spryker/shipment": "^8.0.2",
    "spryker/quote": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "fond-of-oryx/product-country-restriction-checkout-connector": "^1.1.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/",
      "Orm\\": "src/Orm/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
