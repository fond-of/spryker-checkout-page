{
  "name": "fond-of-spryker/checkout-page",
  "description": "Customise checkout process",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "spryker/glossary": "^3.8.0",
    "spryker/kernel": "^3.42.4",
    "spryker/zed-request": "^3.12.0",
    "spryker/customer": "^7.26.5",
    "spryker-shop/checkout-page": "^3.7.0",
    "spryker-shop/customer-page": "^2.7.1",
    "spryker/shipment": "^8.0.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "codeception/codeception": "<4.1.2",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
  }
}
